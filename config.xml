		<!-- Splash Screens -->
		<gap:splash src="resource/splash/480x800.png" gap:platform="android" width="480" height="800" />
		<gap:splash src="resource/splash/200x320.png" gap:platform="android" width="200" height="320" />
		<gap:splash src="resource/splash/512x512_7.png" gap:platform="android" width="320" height="480" />
		<gap:splash src="resource/splash/720x1280.png" gap:platform="android" width="720" height="1280" />
		<gap:splash src="resource/splash/800x480.png" gap:platform="android" width="800" height="480" />
		<gap:splash src="resource/splash/320x200.png" gap:platform="android" width="320" height="200" />
