<?xml version='1.0' encoding='utf-8'?>
    <widget xmlns:gap="http://phonegap.com/ns/1.0" 
    xmlns="http://www.w3.org/ns/widgets" 
    id="com.staticking.com" 
    version="2.0.1">

        <name>Staticking</name>
        <description>
           Bulk sms
        </description>
        <author email="rvgupta444@gmail.com"
        href="http://cordova.io">
           Staticking  team
        </author>
    
    	<plugin name="org.apache.cordova.console" spec="0.2.11"/>
    	<plugin name="org.apache.cordova.device" spec="0.2.3" />
    	
    	
		 <plugin name="org.apache.cordova.inappbrowser"   />
		
    
    	<plugin name="cordova-plugin-contacts" spec="~2.3.0" />
        
		 <plugin name="com.telerik.plugins.nativepagetransitions" source="npm" />
		
		 <plugin name="org.apache.cordova.file" spec="1.3.1"  />
		 <plugin name="cordova-plugin-network-information" spec="~1.3.2" />
		 <plugin name="cordova-plugin-dialogs" spec="~1.3.2" />
		 
		<plugin name="org.apache.cordova.contacts" spec="~1.3.2" />
		
		<plugin name="cordova-plugin-sms" source="npm" />
		
		<plugin name="cordova-plugin-splashscreen" source="npm" />
		
  	<!--	<gap:plugin name="nl.x-services.plugins.googleplus" source="plugins.cordova.io"/> -->
  		
    	<!--Icons and Splash Screens Configuration for Mofluid Apps-->
		
		<!-- ===================== -->
		<!-- Global Configuration  -->
		<!-- ===================== -->
		<!-- Icons -->
		<icon src="resource/icon/96x96px_2.png" />
		<!-- Splash Screens -->
	
		<!-- ====================== -->
		<!-- iOS Configuration      -->
		<!-- ====================== -->
		<!-- Icons -->
		
		
		
		<icon src="resource/icon/96x96px_2.png" gap:platform="android" width="96" height="96" />
		<icon src="resource/icon/72x72px_1.png" gap:platform="android" width="72" height="72" />
		<icon src="resource/icon/48x48px_1.png" gap:platform="android" width="48" height="48" />
		<icon src="resource/icon/36x36px_1.png" gap:platform="android" width="36" height="36" />
		<icon src="resource/icon/96x96px_3.png" gap:platform="android" width="96" height="96" />
		
	
		
			<platform name="android">
		   
			<splash src="res/screen/android/splash-land-hdpi.png" density="land-hdpi"/>
			<splash src="res/screen/android/splash-land-ldpi.png" density="land-ldpi"/>
			<splash src="res/screen/android/splash-land-mdpi.png" density="land-mdpi"/>
			<splash src="res/screen/android/splash-land-xhdpi.png" density="land-xhdpi"/>

			<splash src="res/screen/android/splash-port-hdpi.png" density="port-hdpi"/>
			<splash src="res/screen/android/splash-port-ldpi.png" density="port-ldpi"/>
			<splash src="res/screen/android/splash-port-mdpi.png" density="port-mdpi"/>
			<splash src="res/screen/android/splash-port-xhdpi.png" density="port-xhdpi"/>
		   </platform>
		   
	         <preference name="phonegap-version" value="cli-6.5.0" /> 
			
			 <preference name="SplashScreenDelay" value="50000" />
			  <preference name="AutoHideSplashScreen" value="true" />
			
				  <preference name="disable-cursor" value="false"/>
                  <preference name="android-minSdkVersion" value="14"/>
                <preference name="orientation" value="portrait" />
                <preference name="target-device" value="universal" />
                <preference name="DisallowOverscroll" value="true" />
          	<preference name="webviewbounce" value="false" />
			
				<preference name="OverrideUserAgent" value="Mozilla/5.0 Google" />
			
     <plugin name="cordova-plugin-whitelist" version="1"/>
        

 <content src="index.html" />
    	<access origin="*" />
	</widget>
